
${template convert-}
${if helper.isNode(this)-}
${if helper.containsColon(tagName)-}
${call(`helper.replaceColon(tagName)`) this-}
${else-}
${this.getNodeDeclaration()-}
${call(convertList) children-}
</${tagName}>${end-}
${else-}
${this-}
${end-}
${end}


${template convertList-}
${foreach child in this-}
${call(convert) child-}
${end-}
${end}